{
    "swagger": "2.0",
    "info": {
        "description": "This is the API server for the Blue Banner Engine, providing FRC match predictions.",
        "title": "Blue Banner Engine (BBE) API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "url": "http://www.swagger.io/support",
            "email": "support@swagger.io"
        },
        "license": {
            "name": "MIT",
            "url": "https://opensource.org/licenses/MIT"
        },
        "version": "1.0"
    },
    "basePath": "/api/v1",
    "paths": {
        "/events/{year}": {
            "get": {
                "description": "Retrieves a simplified list of all FRC events for a given year from The Blue Alliance.",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "events"
                ],
                "summary": "Get All Events for a Year",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "FRC Year (e.g., 2024)",
                        "name": "year",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/routes.TbaEvent"
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/routes.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/predict/event/{event_key}": {
            "get": {
                "description": "Retrieves all match predictions for a given FRC event key from the BBE prediction service.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "predictions"
                ],
                "summary": "Get All Match Predictions for an Event",
                "parameters": [
                    {
                        "type": "string",
                        "description": "FRC Event Key (e.g., 2025mxle)",
                        "name": "event_key",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful prediction for all matches",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/types.MatchPrediction"
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error (e.g., gRPC call failed)",
                        "schema": {
                            "$ref": "#/definitions/routes.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/predict/event/{event_key}/playoff/{n_sims}": {
            "get": {
                "description": "Using matchpoint predictor, simulates the playoff results for an event",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "predictions"
                ],
                "summary": "Get the playoff simulated n times for an Event",
                "parameters": [
                    {
                        "type": "string",
                        "description": "FRC Event Key (e.g., 2025mxle)",
                        "name": "event_key",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "description": "Number of desired simulations (e.g., 1000)",
                        "name": "n_sims",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful prediction for all matches",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/types.SimulationResponse"
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error (e.g., gRPC call failed)",
                        "schema": {
                            "$ref": "#/definitions/routes.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/predict/match/{match_key}": {
            "get": {
                "description": "Retrieves a detailed prediction for a single FRC match, including SHAP analysis for model explainability.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "predictions"
                ],
                "summary": "Get Single Match Prediction with SHAP",
                "parameters": [
                    {
                        "type": "string",
                        "description": "FRC Match Key (e.g., 2025mxle_qm12)",
                        "name": "match_key",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful prediction",
                        "schema": {
                            "$ref": "#/definitions/types.MatchPrediction"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error (e.g., gRPC call failed)",
                        "schema": {
                            "$ref": "#/definitions/routes.ErrorResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "routes.ErrorResponse": {
            "type": "object",
            "properties": {
                "error": {
                    "type": "string"
                }
            }
        },
        "routes.TbaEvent": {
            "type": "object",
            "properties": {
                "city": {
                    "type": "string"
                },
                "country": {
                    "type": "string"
                },
                "end_date": {
                    "type": "string"
                },
                "event_code": {
                    "type": "string"
                },
                "event_type": {
                    "type": "integer"
                },
                "key": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "start_date": {
                    "type": "string"
                },
                "state_prov": {
                    "type": "string"
                }
            }
        },
        "types.MatchPrediction": {
            "type": "object",
            "properties": {
                "actual_scores": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "integer"
                    },
                    "example": {
                        "blue": 98,
                        "red": 100
                    }
                },
                "actual_winner": {
                    "type": "string",
                    "example": "red"
                },
                "match_key": {
                    "type": "string",
                    "example": "2025mxle_qm1"
                },
                "predicted_scores": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "integer"
                    },
                    "example": {
                        "blue": 112,
                        "red": 95
                    }
                },
                "predicted_winner": {
                    "type": "string",
                    "example": "blue"
                },
                "shap_analysis": {
                    "$ref": "#/definitions/types.ShapAnalysis"
                },
                "status": {
                    "type": "string",
                    "example": "played"
                },
                "team_keys": {
                    "$ref": "#/definitions/types.TbaTeamKeys"
                },
                "win_probability": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "number"
                    },
                    "example": {
                        "blue": 0.79,
                        "red": 0.21
                    }
                }
            }
        },
        "types.Result": {
            "type": "object",
            "properties": {
                "alliance_number": {
                    "type": "integer"
                },
                "teams": {
                    "type": "array",
                    "items": {
                        "type": "integer"
                    }
                },
                "win_probability": {
                    "type": "number"
                },
                "wins": {
                    "type": "integer"
                }
            }
        },
        "types.ShapAnalysis": {
            "type": "object",
            "properties": {
                "base_value": {
                    "type": "number"
                },
                "feature_data": {
                    "type": "array",
                    "items": {
                        "type": "number"
                    }
                },
                "feature_names": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "values": {
                    "type": "array",
                    "items": {
                        "type": "number"
                    }
                }
            }
        },
        "types.SimulationMetadata": {
            "type": "object",
            "properties": {
                "timestamp_utc": {
                    "type": "string"
                },
                "total_simulations_run": {
                    "type": "integer"
                }
            }
        },
        "types.SimulationResponse": {
            "type": "object",
            "properties": {
                "event_key": {
                    "type": "string"
                },
                "results": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/types.Result"
                    }
                },
                "simulation_metadata": {
                    "$ref": "#/definitions/types.SimulationMetadata"
                }
            }
        },
        "types.TbaTeamKeys": {
            "type": "object",
            "properties": {
                "blue": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "red": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }
        }
    }
}